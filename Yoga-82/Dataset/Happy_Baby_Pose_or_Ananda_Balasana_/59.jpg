<script type="text/javascript">
  (function () {
    let swiper = undefined
    const runSlider = (id) => {
      const settings = {
        slidesPerView: 1.15,
        spaceBetween: 16,
        loop: false, // Not recommended to enable!!!
        preloadImages: false,
        navigation: false,
        // Enable lazy loading
        lazy: true,
        pagination: {
          el: id + '-pagination',
        },
        breakpoints: {
          550: {
            slidesPerView: 2,
          }
        }
      }
      swiper = new Swiper(id + '-tiled-gallery-slider', {
        ...settings
      });
    }
    const switchSwiper = (id) => {
      if (window.innerWidth < 1024) {
        if (swiper === undefined) {
          runSlider(id)
        }
      } else {
        if (swiper !== undefined) {
          swiper.destroy(true, true)
          swiper = undefined
        }
      }
    }
    switchSwiper('#el-tiled-gallery-lyra')
    window.addEventListener("resize", () => switchSwiper('#el-tiled-gallery-lyra'));
  })();
</script>

